{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "generateblocks-pro/accordion",
  "title": "Accordion",
  "category": "generateblocks",
  "description": "Show and hide content within an accordion.",
  "keywords": [
    "accordion"
  ],
  "version": "1.0.0",
  "textdomain": "generateblocks-pro",
  "attributes": {
    "uniqueId": {
      "type": "string",
      "default": ""
    },
    "tagName": {
      "type": "string",
      "default": "",
      "enum": [
        "div",
        "section",
        "aside",
        "nav",
        "ul",
        "ol",
        "li"
      ]
    },
    "styles": {
      "type": "object",
      "default": {}
    },
    "css": {
      "type": "string",
      "default": ""
    },
    "globalClasses": {
      "type": "array",
      "default": []
    },
    "htmlAttributes": {
      "type": "object",
      "default": {}
    }
  },
  "supports": {
    "align": false,
    "className": false
  },
  "editorStyle": [
    "file:./index.css"
  ],
  "editorScript": [
    "file:./index.js"
  ],
  "providesContext": {
    "generateblocks/accordion/htmlAttributes": "htmlAttributes"
  }
}